{
    "name": "kr37/drycalendar",
    "description": "Calendar plugin with unrestricted repetition for Craft CMS 3",
    "type": "craft-plugin",
    "version": "3.0.0-alpha+20180429",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "drycalendar"
    ],
    "support": {
        "docs": "https://github.com/kr37/drycalendar",
        "issues": "https://github.com/kr37/drycalendar"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "KR37",
            "homepage": "https://github.com/kr37/drycalendar"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1"
    },
    "autoload": {
        "psr-4": {
          "kr37\\drycalendar\\": "src/"
        }
    },
    "extra": {
        "name": "DryCalendar",
        "handle": "drycalendar",
        "hasCpSettings": true,
        "hasCpSection": true,
        "changelogUrl": "https://github.com/kr37/drycalendar",
        "class": "kr37\\drycalendar\\DryCalendar"
    }
}
